{
  "name": "apple-music",
  "version": "0.1.0",
  "description": "Apple Music plugin for WordPress",
  "repository": "git+https://github.com/alleyinteractive/apple-music.git",
  "author": "Alley Interactive",
  "license": "GPL-2.0-or-later",
  "scripts": {
    "build": "webpack --config webpack.config.js --env.production",
    "watch": "webpack --config webpack.config.js --env.dev --watch"
  },
  "devDependencies": {
    "@wordpress/babel-preset-default": "^1.1.3",
    "autoprefixer": "^8.3.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "postcss": "^6.0.21",
    "style-loader": "^0.21.0",
    "stylelint-order": "^0.8.1",
    "url-loader": "^1.0.1",
    "webpack": "^3.11.0"
  },
  "dependencies": {
    "postcss-cssnext": "^3.1.0",
    "postcss-discard-comments": "^2.0.4",
    "postcss-focus": "^3.0.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.4",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^3.0.0",
    "postcss-reporter": "^5.0.0",
    "postcss-units": "^1.1.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "stylelint": "^9.2.0"
  },
  "babel": {
    "presets": [
      "@wordpress/default"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./"
          ],
          "alias": {
            "API": "./blocks/apple-music/api",
            "Components": "./blocks/apple-music/components",
            "Config": "./blocks/apple-music/config",
            "Images": "./assets/images",
            "Utils": "./blocks/apple-music/utils"
          }
        }
      ]
    ]
  }
}
